#
#  SPARC/SIS Support 
#

runARGS()
{
  echo "-m 64 ${1}"
}

checkBSPFaults()
{
  logfile=$1
  grep "^Memory exception at " ${logfile}
  badAccessExit=$?
  if [ $badAccessExit -eq 0 ] ; then
    return 1
  fi
  return 0
}

bspLimit()
{
  testname=$1
  case ${testname} in
    *)         limit=180 ;;
  esac
  echo ${limit}
}
